/*
 * Other definitions for gcc/g++.
 */

     GPLUSPLUS_INCLUDE_DIR = GPlusPlusIncludeDir
          TOOL_INCLUDE_DIR = ToolIncludeDir

#ifndef DependCCFlags
#if 1 /* without -D__GNUG__ */
#define DependCCFlags -DMAKEDEPEND $(CCDEFINES) $(CCINCLUDES) -I$(GPLUSPLUS_INCLUDE_DIR) \
		      -I$(TOOL_INCLUDE_DIR)
#else /* with -D__GNUG__ */
#define DependCCFlags -D__GNUG__ -DMAKEDEPEND $(CCDEFINES) $(CCINCLUDES) -I$(GPLUSPLUS_INCLUDE_DIR) \
		      -I$(TOOL_INCLUDE_DIR)
#endif
#endif

#undef HasGcc2
#define HasGcc2 YES

#undef HasGcc
#define HasGcc HasGcc2
