XCOMM ComGlyph - InterViews-3.1-derived glyphs for Attribute editing
XCOMM

PACKAGE = ComGlyph

#ifdef InObjectCodeDir

LIB = ComGlyph

Use_libInterViews()

MakeLibrary($(LIB),$(VERSION))

#define Obj26(file) MakeObjectFromSrcFlags(file,)
#define Obj31(file) MakeObjectFromSrcFlags(file, -Uiv2_6_compatible -I$(TOP)/src/include $(TOP_CCINCLUDES))

Obj31(attrdialog)
Obj31(terpdialog)

IncludeDependencies()

#else

MakeInObjectCodeDir()
InstallIncludes($(PACKAGE))

#endif
