depend::
install.man:: install

FORMATTED	= libgraphic.PS
TROFFFLAGS	= -man
TROFFSRCS	= \
	libgraphic.n \
	Damage.n \
	Graphic.n \
	GraphicBlock.n \
	Persistent.n

print:
	$(TROFF) $(TROFFFLAGS) $(TROFFSRCS)

format: $(FORMATTED)

$(FORMATTED): $(TROFFSRCS)
	$(RM) $(FORMATTED)
	$(TROFF) -t $(TROFFFLAGS) $(TROFFSRCS) > $(FORMATTED)

clean::
	$(RM) $(FORMATTED)

install:: $(TROFFSRCS)
	@for i in $(TROFFSRCS); do \
		(set -x; $(INSTALL) -c $(INSTMANFLAGS) $$i $(LIBMANDIR)); \
	done

install:: $(TROFFSRCS)
	@for i in $(TROFFSRCS); do \
		(set -x; $(INSTALL) -c $(INSTMANFLAGS) $$i $(RELATIVEMANDIR)); \
	done

