\documentstyle{helvetica}
\documentstyle{18pt}
\documentstyle{mediumpage}

%baselineskip 0.1
%baselinestretch 0
%parskip 0.1
%parstretch 0
%bigskip 3.0
%bigstretch 0
%medskip 2.0
%medstretch 0
%smallskip 1.0
%smallstretch 0
%columns 1

%floatrepelright 0.1in
%floatrepelleft 0.1in
%floatrepeltop 0.25in
%floatrepelbottom 0.25in

%macro{title}{\bf\LARGE\align{center}#}
%macro{section}{\bigskip\bf\Large#\medvspace}
%macro{section*}{\bigskip\bf\Large#\medvspace}
%macro{subsection}{\medskip\bf\large#\smallvspace}

%macro{enumerated}{\medskip\bullet\quad#}
%macro{itemized}{\medskip\bullet\quad#}

%macro{figurecaption}{\align{center}\smallskip #\smallskip}
%macro{tablecaption}{\align{center}\smallskip #\smallskip}
