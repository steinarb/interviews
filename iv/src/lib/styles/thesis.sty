\documentstyle{times}
\documentstyle{12pt}
\documentstyle{mediumpage}

%topmargin 0.6in
%bottommargin 0.5in

%baselineskip 0.5
%baselinestretch 0
%parskip 1.0
%parstretch 1.0
%bigskip 3.0
%bigstretch 2.0
%medskip 2.0
%medstretch 1.0
%smallskip 1.5
%smallstretch 0.5
%columns 1

%floatrepelright 0.1in
%floatrepelleft 0.1in
%floatrepeltop 0.25in
%floatrepelbottom 0.25in

%hyphenpenalty 50
%parbreakpenalty -500
%skippenalty -1000
%linepenalty 10
%pagepenalty 100

%counter{chapter}[arabic]
%counter{section}{chapter}[arabic]
%counter{subsection}{section}[arabic]
%counter{figure}{chapter}[arabic]
%counter{table}{chapter}[arabic]
%counter{reference}[arabic]

%macro{chapter}{\bigskip\counter{chapter}{\bf\LARGE Chapter~\ref{chapter}\smallskip#\hfil}\bigvspace}
%macro{section}{\bigskip\counter{section}\bf\Large\ref{section}\hfill\parbox{-50pt}{#\hfil}\medvspace}
%macro{section*}{\bf\Large\parbox{-0.1pt}{#\hfil}\medvspace}
%macro{subsection}{\medskip\counter{subsection}\bf\large\ref{subsection}\hfill\parbox{-50pt}{#\hfil}\smallvspace}
%macro{subsubsection}{\medskip\bf\large #\hfil\\\}

%macro{enumerated}{\bullet\hfill\parbox{-15pt}{#\hfil}\smallskip}
%macro{itemized}{\bullet\hfill\parbox{-15pt}{#\hfil}\smallskip}
%macro{referenced}{\counter{reference}[\ref{reference}]\hfill\parbox{-40pt}{#\hfil}\smallskip}

%macro{figurecaption}{\align{center}\counter{figure}\smallskip Figure~\ref{figure}:\quad#\smallskip}
%macro{tablecaption}{\align{center}\counter{table}\smallskip Table~\ref{table}:\quad#\smallskip}
