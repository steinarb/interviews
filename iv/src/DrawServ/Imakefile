MakeLibrary($(LIB),$(VERSION))

#define Obj26(file) MakeObjectFromSrcFlags(file,)
#define Obj26A(file) MakeObjectFromSrcFlags(file, -D__ACE_INLINE__)

#ifdef AceEnabled
