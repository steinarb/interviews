#
# include files for using predefined bitmaps
#

HEADERS = *.bm
DEST = $(INCDIR)/InterViews/Bitmaps

install:: $(HEADERS)
	-@if [ -d $(DEST) ]; then exit 0; \
	else (set -x; $(MKDIRHIER) $(DEST)); fi
	@for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTINCFLAGS) $$i $(DEST)); \
	done

