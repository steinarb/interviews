/**/#
/**/# Definitions for building g++/gcc
/**/#

#if defined(MipsArchitecture)
#define GCC_Target mips
#define GCC_TargetMachine mips
#endif

#if defined(SparcArchitecture)
#define GCC_Target sparc
#define GCC_TargetMachine sparc
#endif

#if defined(SunArchitecture) && defined(mc68020)
#define GCC_Target machinedep
#define GCC_TargetMachine m68k
#endif

#ifndef GCC_Target
#define GCC_Target machinedep
#define GCC_TargetMachine machinedep
#endif

TARGET = GCC_Target
GCC_TM = GCC_TargetMachine
