	handMask.bm \
	lfast.bm \
	lfastMask.bm \
	lmoverHit.bm \
	lmoverMask.bm \
	lmover.bm \
	radioBoth.bm \
	radioChosen.bm \
	radioHit.bm \
	radioMask.bm \
	radio.bm \
	reducerHit.bm \
	reducerMask.bm \
	reducer.bm \
	rfast.bm \
	rfastMask.bm \
	rmoverHit.bm \
	rmoverMask.bm \
	rmover.bm \
	ufast.bm \
	ufastMask.bm \
	umoverHit.bm \
	umoverMask.bm \
	umover.bm 

MakeDirectories(install,$(INCDIR)/Bitmaps)
